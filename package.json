{
  "name": "spotify-wrapper-tdd",
  "version": "1.0.0",
  "description": "A wrapper to work with the (Spotify Web API)(https://developer.spotify.com/documentation/web-api/).",
  "main": "src/index.js",
  "scripts": {
    "clear": "rimraf rm -rf lib",
    "build": "babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "build:umd": "webpack --output-filename spotify-wrapper.umd.js",
    "build:umd:min": "webpack --output-filename spotify-wrapper.umd.min.js --mode production",
    "build:all": "npm run build && npm run build:umd && npm run build:umd:min",
    "lint": "eslint src/*.js",
    "prepush": "npm run lint",
    "watch": "npm run build -- --watch",
    "start": "webpack-dev-server --inline --open",
    "test": "mocha --require babel-register",
    "test:tdd": "mocha --require babel-register --watch",
    "test:coverage": "nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/annecarv/spotify-wrapper-tdd"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Anne Carvalho",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.18.6",
    "babel-loader": "^8.2.5",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.19",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "sinon-stub-promise": "^4.0.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "dependencies": {
    "chai": "^4.3.6",
    "npm": "^8.13.2"
  }
}
